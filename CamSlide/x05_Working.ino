void run()
{
	int runtime = 200;

	lcd.clearDisplay();
	printString(lcdRun01, 0, 0);
	printString(lcdRun02, 0, 2);
	printBuffer(runtime, 3, 4);
	printString(lcdUnitSecond, 8, 4);
	printString(lcdRun03, 0, 6);
	printString(lcdRun04, 0, 7);
	delay(5000);
	lcd.setDisplayOff();

	delay(2000);

	lcd.setDisplayOn();
}
